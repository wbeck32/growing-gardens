{
  "name": "kickstart-js",
  "version": "1.0.0",
  "description": "Example on using create-react-app with a Node Express Backend",
  "author": {
    "name": "Katie Boyd",
    "twitter": "@katrilllion",
    "url": "https://jobs.nike.com/category/technology-jobs/15675/29758/1"
  },
  "license": "MIT",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "dev:server": "cd client && yarn build && cd .. && yarn start",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "@google-cloud/translate": "^2.1.3",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.18.3",
    "eslint": "^5.12.0",
    "express": "^4.17.3",
    "jshint": "^2.9.7",
    "nodemon": "^1.18.9",
    "react-dom": "^16.7.0",
    "react-script": "^2.0.5",
    "react-scripts": "^2.1.3",
    "yarn": "^1.13.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-destructuring": "^7.2.0",
    "@babel/plugin-transform-spread": "^7.2.2",
    "@babel/plugin-transform-template-literals": "^7.2.0",
    "babel-preset-es2016": "^6.24.1",
    "concurrently": "^4.1.0",
    "eslint-plugin-react": "^7.12.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/esausilva/example-create-react-app-express.git"
  },
  "bugs": {
    "url": "https://github.com/esausilva/example-create-react-app-express/issues"
  },
  "keywords": [
    "node",
    "express",
    "create-react-app"
  ]
}
